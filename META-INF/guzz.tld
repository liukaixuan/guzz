<?xml version="1.0" encoding="UTF-8" ?>

<taglib xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd"
    version="2.0">
    
  <description>guzz taglib。通过guzz框架进行数据库查询和更新操作。</description>
  <display-name>Guzz Taglib</display-name>
  <tlib-version>1.1</tlib-version>
  <short-name>g</short-name>
  <uri>http://www.guzz.org/tags</uri>
  
  <tag>
    <description>
		用于为list/get/page等数据读取标签，设置查询条件。
    </description>
    <name>boundary</name>
    <tag-class>org.guzz.taglib.db.GhostBoundaryTag</tag-class>
    <body-content>JSP</body-content>
    <attribute>
        <description>
			Condition
        </description>
        <name>limit</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>java.lang.Object</type>
    </attribute>
    <attribute>
        <description>
			set all limit conditons until now to the atrribute var of the scope
        </description>
        <name>var</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			scope. default scope is request
        </description>
        <name>scope</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			tableCondition used to shadow table. this attribute's not-null value will override Guzz.setTableCondtion(xxx). The boundary's tableCondtion also affects the g:inc tag insided.
        </description>
        <name>tableCondition</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			is parent taglib's condition took into this tag?
        </description>
        <name>inherit</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>boolean</type>
    </attribute>
  </tag>
  
  <tag>
    <description>
		add a condition to the directly parent boundary.
    </description>
    <name>addLimit</name>
    <tag-class>org.guzz.taglib.db.GhostAddLimitTag</tag-class>
    <body-content>JSP</body-content>
    <attribute>
        <description>
			If the test is true, add the conditon, or ignore it. Default is true.
        </description>
        <name>test</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>boolean</type>
    </attribute>
    <attribute>
        <description>
			Condition
        </description>
        <name>limit</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
		<type>java.lang.Object</type>
    </attribute>
    <attribute>
        <description>
			Condition 2
        </description>
        <name>limit2</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>java.lang.Object</type>
    </attribute>
    <attribute>
        <description>
			Condition 3
        </description>
        <name>limit3</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>java.lang.Object</type>
    </attribute>
  </tag>
  
  <tag>
    <description>
		add a InTerm condition to the directly parent boundary.
    </description>
    <name>addInLimit</name>
    <tag-class>org.guzz.taglib.db.GhostAddInLimitTag</tag-class>
    <body-content>JSP</body-content>
    <attribute>
        <description>
			property name 
        </description>
        <name>name</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			possible values. should a an array or a list instance. the size() must be > 0
        </description>
        <name>value</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
		<type>java.lang.Object</type>
    </attribute>
    <attribute>
        <description>
			eg: we have a list of Channels, but the list should the 'id's of all channels (channel.getId()),
			then you would set this attribute to 'id' as to tell the taglib to perform a object property value retrieving.
        </description>
        <name>retrieveValueProp</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			If the test is true, add the conditon, or ignore it. Default is true.
        </description>
        <name>test</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>boolean</type>
    </attribute>
  </tag>
  
  <tag>
    <description>
		执行数据库统计操作。count计算的结果为long类型数字
    </description>
    <name>count</name>
    <tag-class>org.guzz.taglib.db.GhostCountTag</tag-class>
    <body-content>JSP</body-content>
    <attribute>
        <description>
			the business name to load. Or the domain class's full class name.
        </description>
        <name>business</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>    
    <attribute>
        <description>
			tableCondition used to shadow table. this attribute's not-null value will override Guzz.setTableCondtion(xxx).
        </description>
        <name>tableCondition</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			Condition
        </description>
        <name>limit</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>java.lang.Object</type>
    </attribute>
    <attribute>
        <description>
			设置用于select $selectPhrase 的内容。例如可以为：count(id), sum(score) 等；如果不设置则默认使用count(*)
        </description>
        <name>selectPhrase</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			set the count result to var
        </description>
        <name>var</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			scope. default request
        </description>
        <name>scope</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>    
  </tag>
  
  <tag>
    <description>
		get the first object matched the condition.
    </description>
    <name>get</name>
    <tag-class>org.guzz.taglib.db.GhostGetTag</tag-class>
    <body-content>JSP</body-content>
    <attribute>
        <description>
			the business name to load. Or the domain class's full class name.
        </description>
        <name>business</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			tableCondition used to shadow table. this attribute's not-null value will override Guzz.setTableCondtion(xxx).
        </description>
        <name>tableCondition</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			Condition
        </description>
        <name>limit</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>java.lang.Object</type>
    </attribute>
    <attribute>
        <description>
			set the detail result to var
        </description>
        <name>var</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			scope. default request
        </description>
        <name>scope</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>    
  </tag>
  
  <tag>
    <description>
		list the objects matched the condition.
    </description>
    <name>list</name>
    <tag-class>org.guzz.taglib.db.GhostListTag</tag-class>
    <body-content>JSP</body-content>
    <attribute>
        <description>
			the business name to load. Or the domain class's full class name.
        </description>
        <name>business</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			tableCondition used to shadow table. this attribute's not-null value will override Guzz.setTableCondtion(xxx).
        </description>
        <name>tableCondition</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			Condition
        </description>
        <name>limit</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>java.lang.Object</type>
    </attribute>
    <attribute>
        <description>
			set the list result to var
        </description>
        <name>var</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			scope. default request
        </description>
        <name>scope</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			在读取结果前，跳过的记录数。默认为0，表示不跳过任何记录。
        </description>
        <name>skipCount</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>int</type>
    </attribute>
    <attribute>
        <description>
			pageNo, start with 1.
        </description>
        <name>pageNo</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>int</type>
    </attribute>
    <attribute>
        <description>
			pageSize. default 20.
        </description>
        <name>pageSize</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>int</type>
    </attribute>
    <attribute>
        <description>
			排序方式，多个排序用英文的逗号分隔，如：name asc, id desc, friendCount asc 
        </description>
        <name>orderBy</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>    
  </tag>
  
  
  <tag>
    <description>
		get the page of matched objects.
    </description>
    <name>page</name>
    <tag-class>org.guzz.taglib.db.GhostPageTag</tag-class>
    <body-content>JSP</body-content>
    <attribute>
        <description>
			the business name to load. Or the domain class's full class name.
        </description>
        <name>business</name>
        <required>true</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			tableCondition used to shadow table. this attribute's not-null value will override Guzz.setTableCondtion(xxx).
        </description>
        <name>tableCondition</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			Condition
        </description>
        <name>limit</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>java.lang.Object</type>
    </attribute>
    <attribute>
        <description>
			set the page result to var
        </description>
        <name>var</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			scope. default request
        </description>
        <name>scope</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			在读取结果前，跳过的记录数。默认为0，表示不跳过任何记录。
        </description>
        <name>skipCount</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>int</type>
    </attribute>
    <attribute>
        <description>
			pageNo, start with 1.
        </description>
        <name>pageNo</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>int</type>
    </attribute>
    <attribute>
        <description>
			pageSize. default 20.
        </description>
        <name>pageSize</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>int</type>
    </attribute>
    <attribute>
        <description>
			default:pageNo
        </description>
        <name>pageNoParamName</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
        <description>
			pageSpan. default 10.
        </description>
        <name>pageSpan</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>int</type>
    </attribute>
    <attribute>
        <description>
			pageBeforeSpan. default 5.
        </description>
        <name>pageBeforeSpan</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>int</type>
    </attribute>
    <attribute>
        <description>
			pageAfterSpan. default 5.
        </description>
        <name>pageAfterSpan</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
		<type>int</type>
    </attribute>
    <attribute>
        <description>
			排序方式，多个排序用英文的逗号分隔，如：name asc, id desc, friendCount asc 
        </description>
        <name>orderBy</name>
        <required>false</required>
        <rtexprvalue>true</rtexprvalue>
    </attribute>    
  </tag>  
  
